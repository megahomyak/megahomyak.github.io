h1 = color: #{text color in hex};
h2 = color: #{text color in hex};
h3 = color: #{text color in hex};
h4 = color: #{text color in hex};
h5 = color: #{text color in hex};
h6 = color: #{text color in hex};
p = color: #{text color in hex};
body = margin: 0; padding: 10%; background-color: #{background color in hex};
.bigText = font-size: xx-large;
.horizontallyCentered = text-align: center;
.tight = padding: 0; margin: 0;
.centeredContents = display: flex; justify-content: center; align-items: center;
.takesAllSpace = width: 100%; height: 100%;
.centerContentsEvenly = display: flex; justify-content: space-evenly;
.contactMe = background-color: #039BE5; color: white; border-radius: 15px; border: none; box-shadow: rgba(0, 0, 0, 0.35) 0px 5px 15px;
.contactMe:hover = background-color: #015b86;
.contactMe > img, .contactMe > span = vertical-align: middle;
.contactMe > span = font-weight: bold;
.contactMe > img = height: 1em; margin-right: 5px;
.headingContainer = display: flex; align-items: baseline;
