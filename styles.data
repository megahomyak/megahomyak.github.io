body = margin: 0; padding: 10%; background-color: #{background color in hex}; color: #{text color in hex}; word-wrap: anywhere; font-family: sans-serif;
.bigText = font-size: xx-large;
.horizontallyCentered = text-align: center;
.tight = padding: 0; margin: 0;
.centeredContents = display: flex; justify-content: center; align-items: center;
.takesAllSpace = width: 100%; height: 100%;
.headingContainer = display: flex; align-items: baseline; flex-wrap: wrap; margin-bottom: 1em;
.heading = padding: 0; margin: 0;
legend = font-family: sans-serif;
fieldset = border: 5px solid; border-radius: 8px;
